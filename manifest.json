{
  "manifest_version": 2,
  "name": "Productive Pupil",
  "description": "",
  "version": "1.0.0",
  "icons": {"128": "icon_128.png"},
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["blocking.js", "limitedBlocking.js"]
    }
  ],
  "web_accessible_resources": [
        "css/0_EE.css",
        "css/1_EE.css",
        "css/2_EE.css",
        "css/3_EE.css",
        "css/4_EE.css"
    ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "storage"]

}
